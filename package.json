    "bugs": {
        "url": "https://github.com/PharaStarLLC/snowflakeid/issues"
    },
    "homepage": "https://github.com/PharaStarLLC/snowflakeid#readme",
    "devDependencies": {
        "eslint": "^8.26.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.2.2",
        "license-check-and-add": "^4.0.5",
        "prettier": "^2.7.1"
    }
